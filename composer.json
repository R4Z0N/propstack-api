{
  "name": "r4z0n/propstack-api",
  "type": "library",
  "description": "Simple API Integration for Propstack",
  "keywords": [
    "propstack",
    "rest",
    "api"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "ext-curl": "*",
    "guzzlehttp/guzzle": "^6.3 || ^7.8",
    "guzzlehttp/oauth-subscriber": "0.4.*",
    "psr/cache": "^1.0"
  },
  "require-dev": {
    "fzaninotto/faker": "^1.9.1",
    "phpunit/phpunit": "^8.5",
    "symfony/cache": "^5.1"
  },
  "autoload": {
    "psr-4": {
      "R4Z0N\\Propstack\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
